{
    "manifest_version": 2,
    "name": "GitHubHop",
    "version": "0.1",
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": ["content.js"]
      }
    ],
    "permissions": [
      "tabs",
      "storage"
    ],
    "background": {
      "scripts": [
        "listeners/functions.js",
        "listeners/import_clicked.js",
        "listeners/imports_found.js"
      ],
      "persistent": false
    }
  }