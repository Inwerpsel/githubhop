{
    "manifest_version": 2,
    "name": "GitHubHop",
    "version": "0.1",
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": ["content.js"]
      }
    ],
    "permissions": [
      "tabs"
    ],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    }
  }